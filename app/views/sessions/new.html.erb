<h3>Login</h3>

<%= form_tag login_path do %>
 
  <%= text_field_tag :username, "", placeholder: "Username" %> <br>

  <%= password_field_tag :password, "", placeholder: "Password" %> <br>

  <%= submit_tag "Log In" %>


  <%= link_to 'Log in using Google', '/auth/google_oauth2', method: :post %>

<% end %>


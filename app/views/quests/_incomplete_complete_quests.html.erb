<div class="row">
  <div class="col-sm-6">

    <h3 class="title text-center">Incomplete Quests</h3>
    <p class="card-text"><%= render 'quests/incomplete_quests_cards', quests: user.sort_quests_by_incomplete.limit(5) %></p>
  
  </div>

  <div class="col-sm-6">

    <h3 class="title text-center">Completed Quests</h3>
    <p class="card-text"><%= render 'quests/completed_quests_cards', quests: user.sort_quests_by_completed.limit(5) %></p>
  
    
  </div>
</div>